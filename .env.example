# Newsletter Provider API Key
# Get your API key from: https://buttondown.email/settings
BUTTONDOWN_API_KEY=your_buttondown_api_key_here

# Contact Form Integration
# Get your key from: https://formspree.io/
NEXT_PUBLIC_FORMSPREE_KEY=your_formspree_key_here

# Optional: Analytics (Umami)
# Only needed if you want visitor analytics
NEXT_UMAMI_ID=

# Optional: Comments (Giscus)
# Only needed if you want to enable blog comments
# Configure at: https://giscus.app/
NEXT_PUBLIC_GISCUS_REPO=
NEXT_PUBLIC_GISCUS_REPOSITORY_ID=
NEXT_PUBLIC_GISCUS_CATEGORY=
NEXT_PUBLIC_GISCUS_CATEGORY_ID=
