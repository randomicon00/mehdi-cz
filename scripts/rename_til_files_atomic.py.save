set -euo pipefail
cd /home/medvih/Documents/mehdiakikisite_static
npm --version || true
npm run -s build | sed -n '1,160p'
ls -1 data/blog/ai-*.mdx | wc -l
bash -lc 'shopt -s nullglob; files=(data/blog/ai-*.mdx); echo ${#files[@]}; printf "%s\n" "${files[@]}" | head -n 5'
bash -lc 'ls -1 /home/medvih/Documents/mehdiakikisite_static/data/blog | grep -E "^ai-.*\\.mdx$" | wc -l; echo "Sample:"; ls -1 /home/medvih/Documents/mehdiakikisite_static/data/blog | grep -E "^ai-.*\\.mdx$" | head -n 10'
bash -lc 'node -v; corepack enable >/dev/null 2>&1 || true; yarn -v; yarn install --immutable --check-cache; yarn build'
bash -lc 'printf "AI post count: "; ls -1 data/blog | grep -E "^ai-.*\\.mdx$" | wc -l'

DIR = Path(__file__).resolve().parents[1] / 'data' / 'blog'
report = {'renamed': [], 'skipped': [], 'errors': []}

for p in sorted(DIR.glob('til-*.mdx')):
    dest = p.with_name(p.name.replace('til-', '', 1))
    try:
        if dest.exists():
            report['skipped'].append({'src': str(p), 'dest': str(dest), 'reason': 'dest exists'})
            continue
        p.rename(dest)
        report['renamed'].append({'src': str(p), 'dest': str(dest)})
    except Exception as e:
        report['errors'].append({'path': str(p), 'error': str(e)})

out = DIR / '_remove_til_report_atomic.json'
out.write_text(json.dumps(report, indent=2))
print('WROTE', out)
